<?xml version="1.0" encoding="UTF-8"?>
<Configuration>

    <Appenders>
        <Console name="ConsoleOut" target="SYSTEM_OUT">
            <PatternLayout pattern="[%-5level] %d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %c{2} - %msg%n"/>
        </Console>
    </Appenders>

    <Loggers>
        <Root level="OFF">
            <AppenderRef ref="ConsoleOut"/>
        </Root>
    </Loggers>

</Configuration>